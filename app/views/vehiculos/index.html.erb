<p id="notice"><%= notice %></p>

<h1>Vehiculos</h1>

<table>
  <thead>
    <tr>
      <th>Ruedas</th>
      <th>Motor</th>
      <th>Cant pasajeros</th>
      <th>Placa</th>
      <th>Anyo</th>
      <th>Precio</th>
      <th>Observacion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vehiculos.each do |vehiculo| %>
      <tr>
        <td><%= vehiculo.ruedas %></td>
        <td><%= vehiculo.motor %></td>
        <td><%= vehiculo.cant_pasajeros %></td>
        <td><%= vehiculo.placa %></td>
        <td><%= vehiculo.anyo %></td>
        <td><%= vehiculo.precio %></td>
        <td><%= vehiculo.observacion %></td>
        <td><%= link_to 'Show', vehiculo %></td>
        <td><%= link_to 'Edit', edit_vehiculo_path(vehiculo) %></td>
        <td><%= link_to 'Destroy', vehiculo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vehiculo', new_vehiculo_path %>
